{
  "name": "ngx-web-starter",
  "version": "0.0.10",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-proxy": "ng serve --proxy-config proxy.conf.js",
    "app-version": "node ./app.version.js",
    "build": "npm run app-version && ng build --base-href=./",
    "latest-build": "npm run app-version && ng build --configuration=latest --base-href=./",
    "stage-build": "npm run app-version && ng build --configuration=stage --base-href=./",
    "m-build": "npm run app-version && ng build --es5BrowserSupport=false --base-href=./",
    "prod-build": "npm run app-version && ng build --prod --vendor-chunk --base-href=./",
    "mprod-build": "npm run app-version && ng build --prod --vendor-chunk --es5BrowserSupport=false --base-href=./",
    "sourcemap-prod-build": "npm run prod-build -- --source-map",
    "sourcemap-mprod-build": "npm run mprod-build -- --source-map",
    "prod-build-profile": "npm run prod-build -- --profile",
    "mprod-build-profile": "npm run mprod-build -- --profile",
    "dev-serve": "npm run app-version && ng serve -o",
    "latest-serve": "npm run app-version && ng serve --configuration=latest -o",
    "stage-serve": "npm run app-version && ng serve --configuration=stage -o",
    "prod-serve": "npm run app-version && ng serve --prod -o",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "e2e": "ng e2e",
    "lint": "ng lint && npm run app-version",
    "lint-style": "stylelint ./src/**/*.scss",
    "lint-html": "htmlhint \"src\" --config .htmlhintrc",
    "analyze": "npm run lint-html && npm run lint-style && npm run lint",
    "fix-lint": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "fix-stylelint": "prettier-stylelint --write \"src/**/*.{css,scss}\" ",
    "stylelint-check": "stylelint-config-prettier-check",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "prettier-check": "prettier --debug-check --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "accessibility": "pa11y-ci --threshold 250",
    "clean": "rimraf coverage dist tmp documentation",
    "outdated": "npm outdated --depth 0",
    "docs": "compodoc -p src/tsconfig.app.json -n \"ngx-web-starter documentation\"  --theme \"Vagrant\" -s -o ",
    "stats": "npm run prod-build -- --stats-json && webpack-bundle-analyzer ./dist/ngx-web-starter/stats.json",
    "conventional-changelog": "conventional-changelog",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "git-publish": "npm run prod-build && npx angular-cli-ghpages --dir=dist/ngx-web-starter --branch=gh-pages",
    "audit": "npm audit"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0-rc.4",
    "@angular/cdk": "~8.0.0-rc.0",
    "@angular/common": "~8.0.0-rc.4",
    "@angular/compiler": "~8.0.0-rc.4",
    "@angular/core": "~8.0.0-rc.4",
    "@angular/fire": "~5.1.0",
    "@angular/flex-layout": "~7.0.0-beta.24",
    "@angular/forms": "~8.0.0-rc.4",
    "@angular/material": "~8.0.0-rc.0",
    "@angular/platform-browser": "~8.0.0-rc.4",
    "@angular/platform-browser-dynamic": "~8.0.0-rc.4",
    "@angular/router": "~8.0.0-rc.4",
    "@ngrx/effects": "~8.0.0-beta.1",
    "@ngrx/entity": "~8.0.0-beta.1",
    "@ngrx/router-store": "~8.0.0-beta.1",
    "@ngrx/store": "8.0.0-beta.1",
    "@ngrx/store-devtools": "~8.0.0-beta.1",
    "angular-in-memory-web-api": "~0.8.0",
    "firebase": "~5.10.0",
    "hammerjs": "~2.0.8",
    "lodash": "~4.17.0",
    "rxjs": "~6.5.0",
    "tslib": "~1.9.0",
    "zone.js": "~0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0-rc.4",
    "@angular/cli": "~8.0.0-rc.4",
    "@angular/compiler-cli": "~8.0.0-rc.4",
    "@angular/language-service": "~8.0.0-rc.4",
    "@compodoc/compodoc": "~1.1.0",
    "@primer/css": "~12.4.0",
    "@types/angularfire": "~0.8.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.0",
    "@types/node": "~12.0.0",
    "angular-cli-ghpages": "~0.5.0",
    "codelyzer": "~5.0.1",
    "conventional-changelog-cli": "~2.0.10",
    "htmlhint": "~0.11.0",
    "husky": "~2.3.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "ngrx-store-freeze": "~0.2.0",
    "pa11y-ci": "~2.3.0",
    "prettier": "~1.17.0",
    "prettier-stylelint": "~0.4.0",
    "pretty-quick": "~1.11.0",
    "protractor": "~6.0.0",
    "replace-in-file": "~4.1.0",
    "rxjs-tslint-rules": "~4.23.0",
    "rxjs-watcher": "~1.1.0",
    "source-map-explorer": "~1.8.0",
    "stylelint": "~10.0.0",
    "stylelint-config-prettier": "~5.2.0",
    "stylelint-config-primer": "~7.0.0",
    "stylelint-config-recommended-scss": "~3.3.0",
    "stylelint-config-standard": "~18.3.0",
    "stylelint-scss": "~3.6.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.16.0",
    "tslint-angular": "~3.0.0",
    "tslint-config-prettier": "~1.18.0",
    "tslint-eslint-rules": "~5.4.0",
    "tslint-immutable": "~5.5.2",
    "tslint-language-service": "~0.9.0",
    "typescript": "~3.4.0",
    "webpack-bundle-analyzer": "~3.3.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run analyze",
      "pre-push": "npm run analyze"
    }
  }
}
