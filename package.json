{
  "name": "ngx-web-starter",
  "version": "0.0.6",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-proxy": "ng serve --proxy-config proxy.conf.js",
    "app-version": "node ./app.version.js",
    "build": "npm run app-version && ng build --base-href=./",
    "m-build": "npm run app-version && ng build --es5BrowserSupport=false --base-href=./",
    "prod-build": "npm run app-version && ng build --prod --vendor-chunk --base-href=./",
    "mprod-build": "npm run app-version && ng build --prod --vendor-chunk --es5BrowserSupport=false --base-href=./",
    "sourcemap-prod-build": "npm run prod-build -- --source-map",
    "sourcemap-mprod-build": "npm run mprod-build -- --source-map",
    "prod-build-profile": "npm run prod-build -- --profile",
    "mprod-build-profile": "npm run mprod-build -- --profile",
    "dev-serve": "npm run app-version && ng serve -o",
    "prod-serve": "npm run app-version && ng serve --prod -o",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "e2e": "ng e2e",
    "lint": "ng lint && npm run app-version",
    "lint-style": "stylelint ./src/**/*.scss",
    "lint-html": "htmlhint \"src\" --config .htmlhintrc",
    "analyze": "npm run lint-html && npm run lint-style && npm run lint",
    "fix-lint": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "fix-stylelint": "prettier-stylelint --write \"src/**/*.{css,scss}\" ",
    "stylelint-check": "stylelint-config-prettier-check",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "prettier-check": "prettier --debug-check --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "accessibility": "pa11y-ci --threshold 250",
    "clean": "rimraf coverage dist tmp documentation",
    "outdated": "npm outdated --depth 0",
    "docs": "compodoc -p src/tsconfig.app.json -n \"ngx-web-starter documentation\"  --theme \"Vagrant\" -s -o ",
    "stats": "npm run prod-build -- --stats-json && webpack-bundle-analyzer ./dist/ngx-web-starter/stats.json",
    "conventional-changelog": "conventional-changelog",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "git-publish": "npm run prod-build && npx angular-cli-ghpages --dir=dist/ngx-web-starter --branch=gh-pages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.1",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/fire": "~5.1.2",
    "@angular/flex-layout": "~7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/material": "~7.3.1",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngrx/effects": "~7.4.0",
    "@ngrx/entity": "~7.4.0",
    "@ngrx/router-store": "~7.4.0",
    "@ngrx/store": "~7.4.0",
    "@ngrx/store-devtools": "~7.4.0",
    "angular-in-memory-web-api": "~0.8.0",
    "core-js": "~2.6.5",
    "firebase": "~5.9.0",
    "hammerjs": "~2.0.8",
    "lodash": "~4.17.11",
    "rxjs": "~6.4.0",
    "tslib": "~1.9.0",
    "zone.js": "~0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@compodoc/compodoc": "~1.1.0",
    "@primer/css": "~12.2.1",
    "@types/angularfire": "~0.8.33",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.0",
    "angular-cli-ghpages": "~0.5.3",
    "codelyzer": "~5.0.0",
    "conventional-changelog-cli": "~2.0.11",
    "htmlhint": "~0.11.0",
    "husky": "~1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~0.2.2",
    "ngrx-store-freeze": "~0.2.0",
    "pa11y-ci": "~2.1.1",
    "prettier": "~1.16.4",
    "prettier-stylelint": "~0.4.2",
    "pretty-quick": "~1.10.0",
    "protractor": "~6.0.0",
    "replace-in-file": "~3.4.3",
    "rxjs-tslint-rules": "~4.19.0",
    "rxjs-watcher": "~1.1.0",
    "source-map-explorer": "~1.8.0",
    "stylelint": "~9.10.0",
    "stylelint-config-prettier": "~5.0.0",
    "stylelint-config-primer": "~6.0.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.5.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.14.0",
    "tslint-config-prettier": "~1.18.0",
    "tslint-eslint-rules": "~5.4.0",
    "tslint-language-service": "~0.9.9",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "~3.1.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run analyze",
      "pre-push": "npm run analyze"
    }
  }
}
