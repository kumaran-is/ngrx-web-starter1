{
  "name": "ngrx-web-template1",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "app-version": "node ./app.version.js",
    "build": "npm run app-version && ng build --base-href=./",
    "m-build": "npm run app-version && ng build --es5BrowserSupport=false --base-href=./",
    "prod-build": "npm run app-version && ng build --prod --base-href=./",
    "mprod-build": "npm run app-version && ng build --prod --es5BrowserSupport=false --base-href=./",
    "sourcemap-prod-build": "npm run prod-build -- --source-map",
    "sourcemap-mprod-build": "npm run mprod-build -- --source-map",
    "prod-build-profile": "npm run prod-build -- --profile",
    "mprod-build-profile": "npm run mprod-build -- --profile",
    "dev-serve": "npm run app-version && ng serve -o",
    "prod-serve": "npm run app-version && ng serve --prod -o",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:styles": "stylelint ./src/**/*.scss",
    "scss-lint": "scss-lint",
    "e2e": "ng e2e",
    "outdated": "npm outdated --depth 0",
    "doc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -n \"ngrx-web-template1 documentation\"  --theme \"Vagrant\" -s -o ",
    "stats": "ng build --prod --base-href=./ --stats-json && webpack-bundle-analyzer dist/ngrx-web-template1/stats.json",
    "conventional-changelog": "conventional-changelog",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.1",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "~7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/material": "~7.3.1",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngrx/effects": "~7.2.0",
    "@ngrx/entity": "~7.2.0",
    "@ngrx/router-store": "~7.2.0",
    "@ngrx/store": "~7.2.0",
    "@ngrx/store-devtools": "~7.2.0",
    "angular-in-memory-web-api": "~0.8.0",
    "core-js": "~2.6.5",
    "hammerjs": "~2.0.8",
    "rxjs": "~6.4.0",
    "tslib": "~1.9.0",
    "zone.js": "~0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.2",
    "@angular/cli": "~7.3.2",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@compodoc/compodoc": "~1.1.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.0",
    "codelyzer": "~4.5.0",
    "conventional-changelog-cli": "~2.0.11",
    "husky": "~1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~0.2.2",
    "ngrx-store-freeze": "~0.2.4",
    "protractor": "~5.4.2",
    "replace-in-file": "~3.4.3",
    "rxjs-tslint-rules": "~4.17.0",
    "rxjs-watcher": "~1.1.0",
    "source-map-explorer": "~1.7.0",
    "stylelint": "~9.10.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.12.0",
    "tslint-eslint-rules": "~5.4.0",
    "tslint-language-service": "~0.9.9",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "~3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint",
      "pre-push": "ng lint"
    }
  }
}
